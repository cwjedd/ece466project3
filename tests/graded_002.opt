	STR R10, R10, #0
	ADD R10, R5, #-1
	ADD R10, R5, #-2
	STR R10, R10, #0
	ADD R10, R5, #-2
	ADD R10, R5, #-3
	STR R10, R10, #0
	ADD R10, R5, #-3
	ADD R10, R5, #-4
	STR R10, R10, #0
	SET R10, #0
	STR R10, R5, #3
